{
  "name": "zen-web-wallet",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "yarn": "1.9.4"
  },
  "dependencies": {
    "@zen/zenjs": "0.0.24",
    "autoprefixer": "7.1.2",
    "axios": "^0.18.0",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react-app": "^3.0.2",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "6.23.0",
    "bech32": "^1.1.3",
    "bip39": "^2.5.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.6",
    "clipboard-polyfill": "^2.5.4",
    "css-loader": "0.28.4",
    "decimal.js": "^10.0.1",
    "dotenv": "4.0.0",
    "eslint": "4.4.1",
    "eslint-config-react-app": "^2.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "flexbox-react": "^4.4.0",
    "fs-extra": "3.0.1",
    "history": "^4.7.2",
    "html-webpack-plugin": "2.29.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "20.0.4",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "lodash": "^4.17.10",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.5",
    "moment": "^2.22.2",
    "node-sass": "^4.5.3",
    "normalize.css": "^8.0.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "promise": "8.0.1",
    "randomstring": "^1.1.5",
    "rc-checkbox": "^2.1.5",
    "react": "^16.4.2",
    "react-autosuggest": "^9.3.4",
    "react-detect-offline": "^2.1.2",
    "react-dev-utils": "^3.1.0",
    "react-dom": "^16.4.2",
    "react-error-boundary": "^1.2.3",
    "react-error-overlay": "^1.0.10",
    "react-highlight": "^0.12.0",
    "react-idle": "^3.0.0",
    "react-router-dom": "^4.3.1",
    "react-switch": "^3.0.4",
    "sass-loader": "^6.0.6",
    "serve": "^10.0.0",
    "style-loader": "0.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "sw-precache-webpack-plugin": "0.11.4",
    "sweetalert": "^2.1.0",
    "url-loader": "0.5.9",
    "webpack": "3.5.1",
    "webpack-dashboard": "^1.0.0-2",
    "webpack-dev-server": "2.7.1",
    "webpack-manifest-plugin": "1.2.1",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start:prod": "serve build",
    "heroku-postbuild": "yarn build",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "lint": "node node_modules/eslint/bin/eslint.js --ignore-pattern vendor/ --config node_modules/eslint-config-react-app/index.js src",
    "test": "node scripts/test.js --env=jsdom"
  },
  "devDependencies": {
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.13.0",
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.0"
  },
  "lint-staged": {
    "linters": {
      "src/**/*.js": [
        "eslint --config node_modules/eslint-config-react-app/index.js --fix",
        "git add"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "\\.(s?css|styl|less)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
